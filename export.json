      "JAVASCRIPT" : "load('https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.11/handlebars.min.js');\nvar template = Handlebars.compile(html);\nvar shObject = viglet.shiohara.shobject;\n\nvar query  = shObject.query(shContent.system.id, \"PT-ARTICLE\"); // Others > Banner Home\n\nvar forEach = Array.prototype.forEach;\n\n\nvar posts = [];\n\nforEach.call(query, function (shPost) {\n    var post = {\n        \"title\": shPost.TITLE.strValue,\n        \"description\": shPost.DESCRIPTION.strValue,\n    //    \"image\": shObject.generatePostLink(shPost.IMAGE.strValue),\n        \"text\": shPost.TEXT.strValue,\n        \"link\": shObject.generateObjectLink(shPost.id.strValue)\n    }\n    posts.push(post);\n});\n\nvar objects = {\n    \"folder\": shContent,\n    \"posts\":  posts\n}\nvar html = template(objects);\nhtml",
